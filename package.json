{
  "name": "masonite-design-language",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "storybook:vue": "cross-env NODE_PATH=styles:components node_modules/@storybook/vue/bin/index.js -p 9006 -c .storybook/vue",
    "storybook:react": "cross-env NODE_PATH=styles:components node_modules/@storybook/react/bin/index.js -p 9007 -c .storybook/react",
    "build:storybook": "node_modules/@storybook/react/bin/build.js -c .storybook/react -o gh-pages",
    "build-package:external-ui-react": "cd packages/external-ui-react && webpack -c ../../config/webpack.config.js",
    "build-package:svg-icons": "cd packages/svg-icons && npm run build",
    "build-package:tailwindcss": "cd packages/tailwindcss && webpack",
    "publish": "npm run build-package:external-ui-react && lerna publish",
    "lint:scripts": "npx eslint './**/*.js'",
    "lint:scripts:fix": "npm run lint:scripts -- --fix",
    "lint:styles": "npx stylelint **/*.scss --syntax scss",
    "lint": "npm run lint:styles && npm run lint:scripts",
    "format:mdl:source": "prettier-eslint \"@(components|styles)/**/*.@(js|ts|tsx|css|scss)\" --write",
    "format:mdl:storybook": "prettier-eslint \".storybook/**/*.@(js|ts|tsx|css|scss)\" --write",
    "format:package:tailwindcss": "prettier-eslint \"packages/tailwindcss/{!(node_modules)**/*.js,*.js}\" --write",
    "test:watch": "npx jest --watch",
    "test": "npm run lint && npx jest",
    "start": "npm run storybook:react"
  },
  "postinstall": "lerna bootstrap",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "(\\.css|\\.scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/config/setupTests.js",
      "jest-canvas-mock"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@masonite/markup-generator-addon": "^1.0.5",
    "@masonite/svg-icons": "^1.10.0",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-storysource": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/cli": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@storybook/theming": "^6.2.9",
    "@storybook/vue": "^6.2.9",
    "@testing-library/react": "^11.2.2",
    "@types/classnames": "^2.2.11",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^9.1.2",
    "babel-preset-vue": "^2.0.2",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "core-js": "^3.8.1",
    "cross-env": "^5.2.1",
    "css-loader": "^5.2.4",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.3.0",
    "jsdom": "^15.2.1",
    "lerna": "^3.22.1",
    "lottie-react": "^2.1.0",
    "markdown-loader": "^6.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^9.0.2",
    "prettier-eslint-cli": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "16.14",
    "react-datepicker": "^3.8.0",
    "react-dom": "16.14",
    "react-onclickoutside": "^6.9.0",
    "react-scrollbar": "^0.5.6",
    "react-toastify": "^7.0.3",
    "react-transition-group": "^4.4.1",
    "sass": "^1.32.13",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-scss": "^3.18.0",
    "svg-inline-loader": "^0.8.2",
    "tailwindcss": "^2.0.2",
    "tailwindcss-cli": "^0.1.2",
    "tinycolor2": "^1.4.2",
    "typescript": "^4.1.3",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0",
    "webpack-fix-style-only-entries": "^0.6.0"
  }
}
